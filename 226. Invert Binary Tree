/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode(int x) { val = x; }
 * }
 */
public class Solution {
    public TreeNode invertTree(TreeNode root) {
        TreeNode flag;
        if (root==null)
            return root;
        flag = root.left;
        root.left = root.right;
        root.right = flag;
        invertTree(root.left);
        invertTree(root.right);
        return root;
    }
}
